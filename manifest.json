{
    "manifest_version": 3,
  
    "name": "Bookmark+",
    "description": "Advanced bookmarks.",
    "version": "1.0",
  
    "action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },
    "permissions": [
     "activeTab",
     "storage",
     "tabs",
     "scripting"
     ]
  }